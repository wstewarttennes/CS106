import java.io.File;
import java.util.*;




/**
 * @author westonstewarttennes
 *
 */
public class Practice {
	public static void main(String[] args) {
		int x = 10;
		counter(x);
		System.out.println(x);
	}


	public static int counter(int x) {
		if (x > 1) {
			return x - 9;	
		}
		counter(x);
		return 10;
	}
}