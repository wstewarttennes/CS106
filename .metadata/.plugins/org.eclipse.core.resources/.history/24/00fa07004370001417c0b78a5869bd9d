import java.io.File;
import java.util.*;




/**
 * @author westonstewarttennes
 *
 */
public class Practice {
	public static void main(String[] args) {
		int x = 10;
		counter(x);
		System.out.println(x);
	}


	public static int counter(int x) {
		if (x > 0) {
			int y = x - 9;
			x++;
			counter(x);
			return y;	
		}
	}
}