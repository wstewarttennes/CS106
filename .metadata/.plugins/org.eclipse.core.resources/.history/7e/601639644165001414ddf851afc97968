public class Practice {
		public static void main(String[] args) {
			System.out.println("NFC West \t W L T");
			System.out.println("-----------------------");
			System.out.println("Seattle \t 13 3 0");
			System.out.println("San Francisco \t 12 4 0");
			System.out.println("Arizona \t 10 6 0");
			System.out.println("St. Louis \t 7 9 0");
			System.out.println("");
			System.out.println("NFC North \t W L T");
			System.out.println("-----------------------");
			System.out.println("Green Bay \t 8 7 1");
			System.out.println("Chicago \t 8 8 0");
			System.out.println("Detroit \t 7 9 0");
			System.out.println("Minnesota \t 5 10 1");
		
			
		}
}

