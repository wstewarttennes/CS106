import java.io.File;
import java.util.*;




/**
 * @author westonstewarttennes
 *
 */
public class Practice {
	public static void main(String[] args) {
		int x = 10;
		int y;
		counter(x, y);
		System.out.println(y);
	}


	public static int counter(int x, int y) {
		if (x > 1) {
			int y = x - 9;
			return y;	
		}
		y = 0;
		counter(x, y);
		return 10;
	}
}