import java.io.*; //for files
import java.util.*; //for scanner

public class FileReading {

	public static void main(String[] args) throws FileNotFoundException{
		Scanner fileReader = new Scanner(new File("src/input.txt"));
		while (fileReader.hasNextLine()) {
			String text = fileReader.nextLine();
			processLine(text);
		}
		fileReader.close();
		
		
	}
	public static void processLine(String text) {
			Scanner data = new Scanner(text);
			String date = data.next();
			String variable = data.next();
			if (variable != "Checking" && variable != "Savings") {
				if (variable == "Credit") {
					String extraVariable = data.next();
				} else {
					System.out.print("1 planned error");
				}
			}
			String plusOrMinus = data.next();
			if (variable != "Withdrawal" && variable != "Deposit") {
				if (variable == "Opening") {
					String extraVariable = data.next();
				} else {
					System.out.print("2 planned error");
				}
			}
			double value = data.nextDouble();	
			double checking = 0;
			double savings = 0;
			
			double creditCard = 0;
			if (variable == "Checking") {
				 if (plusOrMinus == "Withdrawal") {
					 checking -= value;
				 } else if (plusOrMinus == "Deposit") {
					 checking += value;
				 } else if (plusOrMinus == "Opening Balance") {
					 checking += value;
				 }
			} else if (variable == "Savings") {
				 if (plusOrMinus == "Withdrawal") {
					 savings -= value;
				 } else if (plusOrMinus == "Deposit") {
					 savings += value;
				 } else if (plusOrMinus == "Opening Balance") {
					 savings += value;
				 }
			} else if (variable == "Credit Card") {
				 if (plusOrMinus == "Withdrawal") {
					 creditCard -= value;
				 } else if (plusOrMinus == "Deposit") {
					 creditCard += value;
				 } else if (plusOrMinus == "Opening Balance") {
					 creditCard += value;
				 }
			}
		total(checking, savings, creditCard);
		data.close();
	}
			
	public static void total(double checking, double savings, double creditCard) {
		System.out.println(checking);
		System.out.println(savings);
		System.out.println(creditCard);
		

	}
}
